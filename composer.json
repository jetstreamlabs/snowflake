{
	"name": "jetstreamlabs/snowflake",
	"description": "An ID Generator for PHP based on Snowflake Algorithm (Twitter announced).",
	"license": "MIT",
	"keywords": [
		"snowflake algorithm",
		"php snowflake",
		"laravel snowflake"
	],
	"homepage": "https://github.com/jetstreamlabs/snowflake",
	"authors": [
		{
			"name": "secondman",
			"email": "secondman@jetstreamlabs.com"
		}
	],
	"require": {
		"php": "^8.0",
		"illuminate/database": "^9.0"
	},
	"autoload": {
		"psr-4": {
			"Jetlabs\\Snowflake\\": "src"
		}
	},
	"extra": {
		"laravel": {
			"providers": [
				"Jetlabs\\Snowflake\\Providers\\SnowflakeServiceProvider"
			]
		}
	},
	"require-dev": {
		"jetstreamlabs/pinte": "^1.0"
	}
}
